<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>kortex_description</name>
  <version>0.1.0</version>
  <description>
    <p>Description files for Kinova Gen3 arm and robotiq_2f_85 gripper</p>
    <p>This package also contains configuration data and meshes</p>
  </description>
  <maintainer email="robotogeddon@gmail.com">Abed</maintainer>
  <license>BSD</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <exec_depend>joint_state_publisher</exec_depend>
  <exec_depend>joint_trajectory_controller</exec_depend>
  <exec_depend>robotiq_description</exec_depend>
  <exec_depend>picknik_reset_fault_controller</exec_depend>
  <exec_depend>picknik_twist_controller</exec_depend>
  <exec_depend>rviz2</exec_depend>
  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
